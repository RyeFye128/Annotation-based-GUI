/*
 * Holds the information of a single annotation
 */
public class AnnotationHolder {
	
	private String name, id, type, parentText;
	public String getName() {
		return name;
	}


	public String getValue() {
		return id;
	}
	
	public String getType() {
		return type;
	}
	
	public String getParentText()
	{
		return parentText;
	}


	public AnnotationHolder(String name, String id, String type, String parentText)
	{
		this.type = type;
		this.parentText = parentText;
		this.name = name;
		this.id = id;
	}
}
